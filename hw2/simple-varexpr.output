Grammar

    0 $accept: statement_list $end

    1 statement_list: statement
    2               | statement '\n'
    3               | statement '\n' statement_list
    4               | statement statement_list

    5 statement: expression
    6          | T_NAME '=' expression

    7 expression: expression '+' T_NUMBER
    8           | expression '-' T_NUMBER
    9           | expression '+' T_DOUBLE
   10           | expression '-' T_DOUBLE
   11           | expression '+' T_NAME
   12           | expression '-' T_NAME
   13           | T_NAME
   14           | T_NUMBER
   15           | T_DOUBLE
   16           | '(' expression ')'
   17           | T_EXP '(' expression ')'
   18           | T_SQRT '(' expression ')'
   19           | T_LOG '(' expression ')'


